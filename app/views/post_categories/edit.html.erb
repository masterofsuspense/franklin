<h1>Editing post_category</h1>

<% form_for(@post_category) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @post_category %> |
<%= link_to 'Back', post_categories_path %>
